generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum UserRole {
  Alumni
  Admin
  Developer
  Sponsor
}

model User {
  id        String   @id @default(cuid())
  email     String   @unique
  role      UserRole @default(Alumni)
  firstName String
  lastName  String
}
